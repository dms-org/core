{
    "name": "dms/core",
    "description": "The core library for the dms",
    "authors": [
        {
            "name": "Elliot Levin",
            "email": "elliotlevin@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        { "type": "composer", "url": "http://satis.dev:4321" }
    ],
    "autoload": {
        "psr-4": {
            "Dms\\Core\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "timetoogo/pinq": "~3.2",
        "doctrine/dbal": "~2.5",
        "container-interop/container-interop": "~1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Dms\\Core\\Tests\\": "tests/Tests"
        }
    },
    "require-dev": {
        "dms/common.testing": "dev-master"
    }
}
